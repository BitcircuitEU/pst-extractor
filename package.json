{
  "name": "pst-extractor",
  "version": "1.4.0",
  "description": "Extract objects from MS Outlook/Exchange PST files",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "test": "mocha --opts mocha.opts",
    "test:coverage": "nyc --reporter=html mocha --watch-extensions ts --watch --opts mocha.opts",
    "mintest": "ts-node ./src/test-min.ts",
    "longtest": "ts-node ./src/test.ts"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "src/index.ts",
      "coverage/prettify.js",
      "coverage/sorter.js"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epfromer/pst-extractor.git"
  },
  "keywords": [],
  "author": "Ed Pfromer (epfromer@gmail.com)",
  "license": "(Apache-2.0 OR GPL-3.0)",
  "bugs": {
    "url": "https://github.com/epfromer/pst-extractor/issues"
  },
  "homepage": "https://github.com/epfromer/pst-extractor#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/debug": "4.1.1",
    "@types/long": "^4.0.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.3",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "chai-datetime": "^1.5.0",
    "debug": "^4.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "prettier": "^2.0.5",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.18",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "long": "^4.0.0",
    "uuid-parse": "^1.1.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "endOfLine": "auto"
  }
}
